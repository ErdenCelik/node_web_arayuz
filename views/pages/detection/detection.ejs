<div class="row g-5 g-xl-10 mb-5 mb-xl-10">
    <div class="col-md-12 col-lg-12 col-xl-12 col-xxl-12 mb-md-5 mb-xl-10">
        <div class="card card-flush">
            <div class="card-header pt-5">
                <h3 class="card-title align-items-start flex-column">
                    <span class="card-label fw-bold text-gray-900">Kayıt Geçmişi</span>
                </h3>
            </div>
            <div class="card-body pt-6">
                <div class="d-flex flex-stack flex-wrap mb-5">
                    <div class="d-flex align-items-center position-relative my-1 mb-2 mb-md-0">
                    </div>
                    <div class="d-flex justify-content-end" data-table-detection-toolbar="base">
                        <button type="button" class="btn btn-sm btn-light-primary me-3" data-kt-menu-trigger="click" data-kt-menu-placement="bottom-end">
                            <i class="ki-duotone ki-filter fs-2">
                                <span class="path1"></span>
                                <span class="path2"></span>
                            </i>
                            Filtrele
                        </button>
                        <div class="menu menu-sub menu-sub-dropdown w-300px w-md-325px" data-kt-menu="true" id="filterMenu">
                            <div class="px-7 py-5">
                                <div class="fs-4 text-gray-900 fw-bold">Filtrele Seçenekleri</div>
                            </div>
                            <div class="separator border-gray-200"></div>
                            <div class="px-7 py-5">

                                <div class="mb-5">
                                    <label class="form-label fw-semibold">Kaynak:</label>
                                    <select class="form-select" name="source"
                                            data-kt-select2="true" data-close-on-select="true"
                                            data-placeholder="Kaynak Seçin" data-dropdown-parent="#filterMenu" data-allow-clear="true">
                                        <option></option>
                                        <option value="camera">Kamera</option>
                                        <option value="video">Video</option>
                                    </select>
                                </div>
                                <div class="mb-5">
                                    <label class="form-label fw-semibold">Kaynak Adı:</label>
                                    <div class="input-group">
                                        <input name="sourceName" type="text" class="form-control" placeholder="Kaynak Adı" />
                                    </div>
                                </div>

                                <div class="mb-5">
                                    <label class="form-label fw-semibold">Tarih:</label>
                                    <div class="input-group">
                                        <input id="filterDate" name="filterDate" type="text" class="form-control" placeholder="Tarih" />
                                    </div>
                                </div>

                                <div class="mb-5">
                                    <label class="form-label fw-semibold">Objeler:</label>
                                    <select class="form-select" name="objects[]" multiple
                                            data-kt-select2="true" data-close-on-select="false"
                                            data-placeholder="Objeler Seçin" data-dropdown-parent="#filterMenu" data-allow-clear="true">
                                        <option></option>
                                        <% for (const [key, value] of Object.entries(ObjectTypes)) { %>
                                            <option value="<%= value %>">
                                                <%= ObjectTypesInTurkish[key] %>
                                            </option>
                                        <% } %>
                                    </select>
                                </div>
                                <div class="mb-5">
                                    <label class="form-label fw-semibold">Anomali:</label>
                                    <div class="input-group">
                                        <div class="form-check form-check-custom form-check-solid form-switch">
                                            <input class="form-check-input form-check-sm" type="checkbox" name="anomaly" value="1" />
                                        </div>
                                    </div>
                                </div>

                                <div class="d-flex justify-content-end">
                                    <button type="submit" class="btn btn-sm btn-primary" id="filterApply" data-kt-menu-dismiss="true" data-table-detection-filter="filter">Uygula</button>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="d-flex justify-content-end align-items-center d-none" data-table-detection-toolbar="selected">
                        <div class="fw-bold me-5">
                            <span class="me-2" data-table-detection-select="selected_count"></span> Seçildi
                        </div>
                        <button type="button" class="btn btn-sm btn-danger" data-table-detection-select="delete_selected">
                            Seçilenleri Sil
                        </button>
                    </div>
                </div>



                <table id="datatableDetections" class="table align-middle table-row-dashed fs-6 gy-5">
                    <thead>
                        <tr class="text-start text-gray-500 fw-bold fs-7 text-uppercase gs-0">
                            <th class="w-10px pe-2">
                                <div class="form-check form-check-sm form-check-custom form-check-solid me-3">
                                    <input class="form-check-input" type="checkbox" data-kt-check="true" data-kt-check-target="#datatableDetections .form-check-input" value="1"/>
                                </div>
                            </th>
                            <th>Kaynak</th>
                            <th>Kaynak Adı</th>
                            <th>Nesneler</th>
                            <th>Nesne Sayısı</th>
                            <th>Anomali</th>
                            <th class="w-150px">Görüntü</th>
                            <th class="w-200px">Tarih</th>
                            <th class="text-end w-100px">#</th>
                        </tr>
                    </thead>
                    <tbody class="text-gray-600 fw-semibold">
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>